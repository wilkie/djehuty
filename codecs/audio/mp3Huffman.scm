; wraps reading of the string column of the huffman list
(define list-ref-code
	(lambda (l x)
		(list-ref l (* x 3))
	)
)

; like above, for the x column (middle)
(define list-ref-x
	(lambda (l x)
		(list-ref l (+ (* x 3) 1))
	)
)

; like above, for the y column (right)
(define list-ref-y
	(lambda (l x)
		(list-ref l (+ (* x 3) 2))
	)
)

; creates an array representation of the huffman table
(define huffman-tree-array-impl
	(lambda (l x last-index)
		(cond
			((= x (list-length l))
				'()
			)
			(else
				(append
					(list (list-ref-code l (find-minimum l 0 last-index -1)))
					(huffman-tree-array-impl
						l
						(+ x 1)
						(find-minimum l 0 last-index -1)
					)
				)
			)
		)
	)
)

; converts ALL numbers into strings on an entire depth of a list
(define list-number-to-string
	(lambda (l)
		(cond
			((= (length l) 0)
				'()
			)
			((list? (list-ref l 0))
				(apply
					list
					(map
						(lambda (x)
							(list-number-to-string x)
						)
						l
					)
				)
			)
			(else
				(map
					(lambda (x)
						(string->number x 2)
					)
					l
				)
			)
		)
	)
)

; builds an array representation of the huffman table
(define huffman-tree-array
	(lambda (l)
		(cond
			((= 0 (length l))
				'(())
			)
			((string=? (list-ref-code l 0) "")
				'(())
			)
			(else
				(list-number-to-string (list-by-string-length (huffman-tree-array-impl l 0 -1)))
			)
		)
	)
)

; builds the array of values referred by the array representation
(define huffman-vals-array-impl
	(lambda (l x last-index)
		(cond
			((= x (list-length l))
				'()
			)
			(else
				(append
					(list (list-ref-x l (find-minimum l 0 last-index -1)))
					(list (list-ref-y l (find-minimum l 0 last-index -1)))
					(huffman-vals-array-impl l (+ x 1) (find-minimum l 0 last-index -1))
				)
			)
		)
	)
)

(define list-filter-by-string-length-max
	(lambda (l length)
		(filter
			(lambda (x)
				(>= (string-length x) length)
			)
			l
		)
	)
)

(define list-filter-by-string-length
	(lambda (l length)
		(filter
			(lambda (x)
				(equal? (string-length x) length)
			)
			l
		)
	)
)

; helper to build lists
(define list-by-string-length-impl
	(lambda (l cur-str-length)
		(cond
			((= (length	(list-filter-by-string-length-max l cur-str-length)) 0)
				'()
			)
			(else
				(append
					(list
						(list-filter-by-string-length l cur-str-length)
					)
					(list-by-string-length-impl l (+ cur-str-length 1))
				)
			)
		)
	)
)

; builds lists based on string length
(define list-by-string-length
	(lambda (l)
		(list-by-string-length-impl l 1)
	)
)

; builds the array of values referred by the array representation
(define huffman-vals-array
	(lambda (l)
		(cond
			((= 0 (length l))
				'()
			)
			(else
				(huffman-vals-array-impl l 0 -1)
			)
		)
	)
)

; returns the number of items in the huffman list
(define list-length
	(lambda (l)
		(/ (length l) 3)
	)
)

; converts a binary value into a integer
(define binary-value
	(lambda (str)
		(string->number str 2)
	)
)

; is this item less than the other?
; regards length first and then value
; 000 > 01 > 00 > 1 > 0
(define list-is-less
	(lambda (l item1 item2)
		(cond
			((= item2 -1)
				#t
			)
			((= item1 -1)
				#t
			)
			((< (string-length (list-ref-code l item1)) (string-length (list-ref-code l item2)))
				#t
			)
			((= (string-length (list-ref-code l item1)) (string-length (list-ref-code l item2)))
				(cond
					((< (binary-value (list-ref-code l item1)) (binary-value (list-ref-code l item2)))
						#t
					)
					(else
						#f
					)
				)
			)
			(else
				#f
			)
		)
	)
)

; is this item greater than the other?
; regards length first and then value
; 000 > 01 > 00 > 1 > 0
(define list-is-greater
	(lambda (l item1 item2)
		(cond
			((= item2 -1)
				#t
			)
			((= item1 -1)
				#t
			)
			((> (string-length (list-ref-code l item1)) (string-length (list-ref-code l item2)))
				#t
			)
			((= (string-length (list-ref-code l item1)) (string-length (list-ref-code l item2)))
				(cond
					((> (binary-value (list-ref-code l item1)) (binary-value (list-ref-code l item2)))
						#t
					)
					(else
						#f
					)
				)
			)
			(else
				#f
			)
		)
	)
)

; returns the next smallest value from the huffman table
(define find-minimum
	(lambda (l x compIndex minIndex)
		(cond
			((= x (list-length l))
				minIndex
			)
			((= compIndex x)
				(find-minimum l (+ x 1) compIndex minIndex)
			)
			((list-is-greater l x compIndex)
				(cond
					((list-is-less l x minIndex)
						(find-minimum l (+ x 1) compIndex x)
					)
					(else
						(find-minimum l (+ x 1) compIndex minIndex)
					)
				)
			)
			(else
				(find-minimum l (+ x 1) compIndex minIndex)
			)
		)
	)
)

; helper code for D mixin generation
(define comma-ize
	(lambda (x)
		(string-append
			(make-string (- 5 (string-length (number->string x))) #\space)
			(number->string x)
			", "
		)
	)
)

(define build-part
	(lambda (l)
		(string-append
			(apply
				string-append
				(map comma-ize l)
			)
		)
	)
)

(define build-line
	(lambda (l depth)
		(cond
			((= (length l) 0)
				""
			)
			((< (length l) 10)
				(build-part l)
			)
			(else
				(string-append
					(build-part (take l 10))
					"\n"
					(make-string depth #\tab)
					(build-line (list-tail l 10) depth)
				)
			)
		)
	)
)

; builds a list into an array literal
(define build-D-array-literal-impl
	(lambda (l depth)
		(cond
			((= (length l) 0)
				(string-append
					"["
					"\n"
					(make-string (- depth 1) #\tab)
					"]"
				)
			)
			(else
				(string-append
					"["
					(make-string depth #\tab)
					(cond
						((list? (list-ref l 0))
							(string-append
								(apply
									string-append
									(map
										(lambda (x)
											(string-append
												"\n"
												(make-string depth #\tab)
												(build-D-array-literal-impl x (+ depth 1))
												","
											)
										)
										(drop-right l 1)
									)
								)
								"\n"
								(make-string depth #\tab)
								(build-D-array-literal-impl (last l) (+ depth 1))
							)
						)
						(else
							(string-append
								"\n"
								(make-string depth #\tab)
								(build-line (drop-right l 1) depth)
								(make-string (- 5 (string-length (number->string (last l)))) #\space)
								(number->string (last l))
							)
						)
					)
					"\n"
					(make-string (- depth 1) #\tab)
					"]"
				)
			)
		)
	)
)

; builds a list into an array literal
(define build-D-array-literal
	(lambda (l)
		(build-D-array-literal-impl l 1)
	)
)

; print D code for the array
(define build-D-array
	(lambda (l name type)
		(cond
			((= (length l) 0)
				(string-append
					"const "
					type
					" "
					name
					" = "
					"[]"
					";"
				)
			)
			(else
				(string-append
					"const "
					type
					" "
					name
					" = "
					(build-D-array-literal l)
					";"
				)
			)
		)
	)
)

